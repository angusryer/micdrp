{
  "name": "client",
  "version": "1.0.0",
  "private": true,
  "workspaces": {
    "nohoist": [
      "**"
    ]
  },
  "scripts": {
    "start": "react-native start --reset-cache",
    "android:production": "react-native run-android --variant release",
    "android:staging": "react-native run-android --variant staging",
    "android:development": "react-native run-android --variant debug",
    "ios:production": "react-native run-ios --scheme micdrp",
    "ios:staging": "react-native run-ios --scheme micdrp-staging",
    "ios:development": "react-native run-ios --scheme micdrp-development",
    "test": "jest",
    "lint": "eslint . --ext .js, .jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@xstate/react": "^3.1.2",
    "axios": "^1.2.2",
    "cross-env": "^7.0.3",
    "lodash": "^4.17.21",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-config": "^1.5.0",
    "shared": "1.0.0",
    "xstate": "^4.35.4"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@evilmartians/lefthook": "^1.3.9",
    "@testing-library/react-native": "^11.5.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "commitlint": "^17.5.1",
    "metro-react-native-babel-preset": "0.72.3",
    "react-native-codegen": "^0.71.3",
    "react-test-renderer": "18.1.0"
  }
}
